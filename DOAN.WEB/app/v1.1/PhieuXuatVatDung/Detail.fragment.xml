<core:FragmentDefinition
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core">
	<Dialog id="PXDialog"
        contentWidth="750px" contentHeight="650px" 
        verticalScrolling="false" 
        class="sapUiSizeCompact">
        <customHeader>
        <Toolbar>
            <Title text="Thông tin chi tiết "/>
            <ToolbarSpacer />
            <OverflowToolbarButton  icon="sap-icon://decline" press="onClosePhieuXuatVatDungView"/>
        </Toolbar>
        </customHeader>
        <endButton>
            <Button text="Đóng" press="onClosePhieuXuatVatDungView" />
        </endButton>
        <content>
            <mvc:XMLView viewName="app.PhieuXuatVatDung.Detail" id="app.PhieuXuatVatDung.Detail"/>
        </content>
	</Dialog>
</core:FragmentDefinition>