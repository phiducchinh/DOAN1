<core:FragmentDefinition
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core">
	<Dialog id="HoaDonMuaDetailDialog"
        contentWidth="850px" contentHeight="850px"
        verticalScrolling="false" 
        class="sapUiSizeCompact">
        <customHeader>
        <Toolbar>
            <Title text="Thông tin chi tiết "/>
            <ToolbarSpacer />
            <OverflowToolbarButton  icon="sap-icon://decline" press="onCloseHoaDonMuaView"/>
        </Toolbar>
        </customHeader>
        <endButton>
            <Button text="Đóng" press="onCloseHoaDonMuaView" />
        </endButton>
        <content>
            <mvc:XMLView viewName="app.HoaDonMua.Detail" id="app.HoaDonMua.Detail"/>
        </content>
	</Dialog>
</core:FragmentDefinition>